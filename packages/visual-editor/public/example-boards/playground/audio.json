{
  "title": "Audio",
  "description": "An example of using Gemini Kit's vision(?) node with audio",
  "version": "0.1.0",
  "edges": [
    {
      "from": "input-0",
      "to": "invoke-0",
      "out": "audio",
      "in": "context"
    },
    {
      "from": "input-0",
      "to": "invoke-0",
      "out": "generator",
      "in": "$board"
    },
    {
      "from": "invoke-0",
      "to": "output-0",
      "out": "text",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "input-0",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "audio": {
              "type": "object",
              "behavior": [
                "llm-content"
              ],
              "title": "Audio",
              "properties": {
                "format": {
                  "type": "string",
                  "enum": [
                    "audio-microphone",
                    "audio-microphone"
                  ]
                }
              },
              "required": [
                "format"
              ],
              "additionalProperties": false
            },
            "generator": {
              "type": "object",
              "behavior": [
                "board"
              ],
              "default": "{\n  \"kind\": \"board\",\n  \"path\": \"gemini-generator.json\"\n}",
              "properties": {},
              "required": [],
              "additionalProperties": false
            }
          },
          "required": [
            "audio"
          ]
        }
      }
    },
    {
      "id": "output-0",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": [
                "array",
                "boolean",
                "null",
                "number",
                "object",
                "string"
              ]
            }
          },
          "required": [
            "text"
          ]
        }
      }
    },
    {
      "id": "invoke-0",
      "type": "invoke",
      "configuration": {
        "model": "gemini-1.5-pro-latest",
        "systemInstruction": "Describe what you hear in the audio. Please respond in markdown",
        "text": "unused"
      }
    }
  ]
}