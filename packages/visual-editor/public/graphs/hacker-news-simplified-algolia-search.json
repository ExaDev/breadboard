{
  "title": "Hacker News Angolia Search Simplified",
  "description": "Board which returns story contents using the Hacker News Angolia API",
  "version": "0.1.0",
  "edges": [
    {
      "from": "generator",
      "to": "manipulator",
      "out": "output",
      "in": "object"
    },
    {
      "from": "input-0",
      "to": "generator",
      "out": "searchQuery",
      "in": "query"
    },
    {
      "from": "input-0",
      "to": "generator",
      "out": "searchTags",
      "in": "tags"
    },
    {
      "from": "manipulator",
      "to": "output-0",
      "out": "object",
      "in": "test"
    },
    {
      "from": "query",
      "to": "generator",
      "out": "hackerNewsSearchBoard",
      "in": "$board"
    },
    {
      "from": "query",
      "to": "manipulator",
      "out": "objectManipBoard",
      "in": "$board"
    }
  ],
  "nodes": [
    {
      "id": "input-0",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "searchQuery": {
              "type": "string",
              "title": "Query",
              "description": "The term to search for",
              "examples": [
                "Artificial Intelligence",
                "Machine Learning",
                "Deep Learning"
              ]
            },
            "searchTags": {
              "type": "string",
              "title": "Tags",
              "description": "Filter on a specific tag",
              "enum": [
                "story",
                "comment",
                "poll",
                "pollopt",
                "show_hn",
                "ask_hn",
                "front_page"
              ]
            }
          },
          "required": [
            "searchQuery",
            "searchTags"
          ]
        }
      }
    },
    {
      "id": "query",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "hackerNewsSearchBoard": {
              "type": "object",
              "behavior": [
                "board"
              ],
              "title": "board location",
              "description": "The URL of the generator to call",
              "default": "{\n  \"kind\": \"board\",\n  \"path\": \"hacker-news-algolia-search.json\"\n}",
              "properties": {},
              "required": [],
              "additionalProperties": false
            },
            "objectManipBoard": {
              "type": "object",
              "behavior": [
                "board"
              ],
              "title": "board location",
              "description": "The URL of the generator to call",
              "default": "{\n  \"kind\": \"board\",\n  \"path\": \"object-manipulator.json\"\n}",
              "properties": {},
              "required": [],
              "additionalProperties": false
            }
          },
          "required": []
        }
      }
    },
    {
      "id": "output-0",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "test": {
              "type": [
                "array",
                "boolean",
                "null",
                "number",
                "object",
                "string"
              ]
            }
          },
          "required": [
            "test"
          ]
        }
      }
    },
    {
      "id": "generator",
      "type": "invoke",
      "configuration": {
        "pageNumber": 1,
        "searchLimit": "1"
      }
    },
    {
      "id": "manipulator",
      "type": "invoke",
      "configuration": {
        "keys": [
          "created_at",
          "num_comments",
          "objectID",
          "points",
          "story_id",
          "title",
          "updated_at",
          "url",
          "objectType"
        ],
        "mode": "pick"
      }
    }
  ]
}