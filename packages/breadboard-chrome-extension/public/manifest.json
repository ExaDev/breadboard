{
    "manifest_version": 3,
    "name": "breadboard-chrome-extension",
    "version": "1.0",
    "description": "",
    "action": {
        "default_popup": "index.html",
		"default_title": "Breadboard Summarisation"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab",
		"storage"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
	"icons": {
		"32": "images/icon-32.png"
	}
  }